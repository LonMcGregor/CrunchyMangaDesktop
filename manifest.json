{
  "applications": {
    "gecko": {
      "id": "uk.lonm.crunchyMangaDesktop"
    }
  },
  "manifest_version": 2,
  "name": "Crunchyroll Manga Desktop",
  "version": "1.0",
  "icons": {
    "48": "icons/icon48.png",
    "96": "icons/icon96.png"
  },
    "permissions": [
      "cookies", "*://*.crunchyroll.com/"
    ],
  "content_scripts": [
    {
    "matches": ["*://www.crunchyroll.com/comics/manga/*", "*://www.crunchyroll.com/comics_read/*"],
    "css": ["css/mangaReader.css", "css/mangaList.css"],
    "js": ["scripts/jquery-3.1.0.min.js", "scripts/cmd.js"]
    }
  ],
  "web_accessible_resources": ["scripts/jquery-3.1.0.min.js", "scripts/cmd.js"]
}